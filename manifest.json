{
    "name": "PewDiePie vs T-Series SubGap monitor",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "The extension will continuously monitor the subgap and display it.",
    "icons": {
        "16": "images/16x16.png",
        "19": "images/19x19.png",
        "38": "images/38x38.png",
        "48": "images/48x48.png",
        "96": "images/96x96.png",
        "128": "images/128x128.png"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "19": "images/19x19.png",
            "38": "images/38x38.png",
            "48": "images/48x48.png",
            "96": "images/96x96.png"
        },
        "default_title": "SubGap: No connection",
        "default_popup": "html/index.html"
    },
    "permissions": []
}